{
	"title": "NOAA Month Summary",
	"description": "description.html",
	"time_picker": "month",
	"default_time_span": "this_month",
	"criteria_ui": "criteria.ui",
	"supported_weather_stations": ["generic"],
	"queries": {
		"daily": {
            "db": {
                "file": "daily_psql.sql",
                "parameters": [
                    "heatBase",
                    "coolBase",
                    "start",
                    "end",
                    "fahrenheit",
                    "kmh",
                    "mph",
                    "inches",
                    "stationCode"
                ]
            },
		},
		"month": {
            "db": {
                "file": "month_psql.sql",
                "parameters": [
                    "heatBase",
                    "coolBase",
                    "atDate",
                    "fahrenheit",
                    "kmh",
                    "mph",
                    "inches",
                    "stationCode"
                ]
            },
		},
		"criteria": {
            "db": {
                "file":"criteria_psql.sql"
                "parameters": [
                    "celsius",
                    "kmh",
                    "mph",
                    "inches",
                    "atDate",
                    "title",
                    "city",
                    "state",
                    "altitude",
                    "latitude",
                    "longitude",
                    "altFeet",
                    "coolBase",
                    "heatBase"
                ]
            },
		},
		"high_rain": {
            "db": {
                "file": "high_rain_psql.sql",
                "parameters": [
                    "start",
                    "end",
                    "inches",
                    "stationCode"
                ]
            }
		},
		"counts": {
            "db": {
                "file": "counts_psql.sql",
                "parameters": [
                    "start",
                    "end",
                    "stationCode",
                    "fahrenheit",
                    "inches"
                ]
            },
		}
	},
	"output_type": "show",
	"outputs": [
		{
			"name": "noaamo",
			"title": "NOAA Month",
			"format": "text",
			"template": "noaamo.txt",
			"filename": "NOAAMO.TXT"
		}
	]
}
s
