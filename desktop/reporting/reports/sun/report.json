{
    "title": "Sunrise & Sunset",
	"description": "description.html",
	"icon": "weather.png",
	"time_picker": "datespan",
	"default_time_span": "this_month",
	"dont_prime_cache": true,
	"criteria_ui": "criteria.ui",
	"supported_weather_stations": ["generic"],
	"scripts": ["libs/moment.js", "libs/moment-timezone-with-data-2012-2022.js", "solar_gen.js", "lat_long.js", "solar.js"],
	"output_type": "show",
    	"output_window_size": {
		"height": 750,
		"width": 850
	},
	"outputs": [
		{
			"name": "html",
			"title": "Sunrise && Sunset",
			"icon": "../../icons/report",
			"format": "html",
			"template": "sun.html",
			"filename": "sun.html"
		},
		{
			"name": "text",
			"title": "Plain text",
			"icon": "../../icons/report",
			"format": "text",
			"template": "wlformat.txt",
            "filename": "sun.txt"
		}
	]
}
