{
	"title": "Storm Rain",
	"description": "description.html",
	"time_picker": "timespan",
	"default_time_span": "last_30d",
	"icon": "weather-rain.png",
	"queries": {
		"storm_rain": {
			"web": {
				"file": "sqlite.sql",
				"parameters": [
					"stationCode",
					"start_t",
					"end_t"
				]
			},
			"db": {
				"file": "postgres.sql",
				"parameters": [
					"stationCode",
					"start",
					"end"
				]
			}
		}
	},
	"output_type": "show",
	"output_window_size": {
		"height": 700,
		"width": 600
	},
	"outputs": [
		{
			"name": "html",
			"title": "Storm Rain",
			"icon": "../../icons/report",
			"format": "html",
			"template": "template.html",
			"view_template": "view_template.html",
			"filename": "storm_rain.html"
		},
		{
			"name": "grid",
			"title": "Grid",
			"icon": "../../icons/table",
			"format": "table",
			"query": "storm_rain",
			"filename": "storm_rain.csv",
			"view_columns": {
				"start_time": "Start Time",
				"end_time": "End Time",
				"duration": "Duration",
				"total_rainfall": "Total Rainfall"
			},
			"save_columns": {
				"start_time": "Start Time",
				"end_time": "End Time",
				"duration": "Duration",
				"total_rainfall": "Total Rainfall"
			},
		}
	]
}
